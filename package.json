{
  "name": "deepak-tomar-portfolio",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "rm -rf build && vite build && touch build/.nojekyll && mv build/404/index.html build/404.html",
    "deploy": "npm run build && npx gh-pages -d build -t true",
    "preview": "vite preview",
    "lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. \"**/*.{js,ts,svelte,scss,css,html,md,json}\" && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. \"**/*.{js,ts,svelte,scss,css,html,md,json}\"",
    "prettier": "npm run format",
    "postbuild": "svelte-sitemap --domain https://deepaktomar2032.github.io --trailing-slashes true"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "gh-pages": "^3.2.3",
    "mdsvex": "^0.10.5",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-external-links": "^1.0.0",
    "rehype-slug": "^5.0.0",
    "sass": "^1.80.7",
    "svelte": "^3.54.0",
    "svelte-preprocess": "^4.7.4",
    "svelte-sitemap": "^2.0.1",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "dateformat": "^5.0.3"
  }
}
